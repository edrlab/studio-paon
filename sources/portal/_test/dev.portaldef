			<sm:currentBuilderVersion/>
		</sm:webArtifactsProvider>
	</sm:artifactsProvider>
	<smp:properties>
		<smp:propertiesFile sc:refUri="/portal/_test/dev.props"/>
		<smp:propertiesFile sc:refUri="/portal/_test/_nocommit.props">
